                "tags": [
                    "plentyMarketplace"
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "403": {
                        "description": "There is no plugin associated with the given item ID on plentyMarketplace."
                    }
                },
                "parameters": [
                    {
                        "name": "plentyID",
                        "type": "string",
                        "required": true,
                        "description": "Cookie from a successful login.",
                        "format": "string",
                        "in": "header"
                    },
